# This is Git's per-user configuration file.
[user]
    name = strivetobelazy
    email = strivetobelazy@gmail.com
[core]
	editor = vim
	excludesfile = /Users/lazy/.dotfiles/git/ggitignore
[alias]
	next = !git checkout `git rev-list HEAD..demo-end | tail -1`
	prev = checkout HEAD^
	d = difftool
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[merge]
	tool = code
[mergetool "fugitive"]
	cmd = vim -f -c \"Gdiff\" \"$MERGED\"
	keepBackup = false
